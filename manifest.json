{
  "name": "Github Comment Navigator",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Quickly navigate to comments when viewing code changes",
  "content_scripts": [
    {
      "matches" : [
        "https://github.com/*/pull/*",
        "https://github.com/*/commit/*"
      ],
      "js" : ["comment.js"]
    }
  ]
}
